{
  "name": "woorton",
  "version": "2.0.3",
  "description": "Integration for the Woorton API.",
  "main": "./index.js",
  "module": "./bundle.esm.js",
  "typings": "./index.d.ts",
  "license": "GPL-3.0",
  "publishConfig": {
    "access": "public",
    "directory": "lib"
  },
  "contributors": [
    "Neil Noumsi Fotso <neil@woorton.com>"
  ],
  "keywords": [
    "Woorton"
  ],
  "scripts": {
    "postversion": "cp package.json lib",
    "postbuild": "cp package.json lib",
    "prepare": "yarn build",
    "build": "rimraf lib && tsc --project . && rollup --config",
    "dev": "ts-node-dev --respawn --transpileOnly src/dev",
    "clean": "rimraf lib && rimraf coverage",
    "watch": "rimraf lib && tsc --watch --project . & rollup --config --watch",
    "typecheck": "tsc --project . --noEmit",
    "format": "prettier --write src/{*,**/*}.ts",
    "lint:prettier": "yarn prettier --list-different src/{*,**/*}.ts",
    "lint:tslint": "tsc --noEmit && tslint --config ../../tslint.json src/{*,**/*}.ts",
    "lint": "yarn lint:tslint && yarn lint:prettier",
    "test": "yarn jest --passWithNoTests",
    "test:coverage": "yarn test --coverage",
    "test:watch": "yarn test --watch"
  },
  "devDependencies": {
    "@types/cli-table": "^0.3.0",
    "@types/debug": "^4.1.3",
    "@types/jest": "^24.0.11",
    "@types/node": "^12.0.10",
    "@types/qs": "^6.5.2",
    "@types/ramda": "^0.26.12",
    "@types/ws": "^6.0.3",
    "cp": "^0.2.0",
    "debug": "^4.0.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "rollup": "^1.16.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "ts-jest": "^24.0.2",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.1"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.0",
    "cli-table": "^0.3.1",
    "commander": "^3.0.1",
    "isomorphic-ws": "^4.0.1",
    "qs": "^6.6.0",
    "ramda": "^0.26.1",
    "rxjs": "^6.5.2"
  }
}
